      style={{ marginBottom: \"30px\" }}\r\n      cover={\r\n        !loading && (\r\n          <Image\r\n            src={images && images.length ? images[0].url : \"error\"}\r\n            style={{ height: \"200px\", objectFit: \"cover\" }}\r\n            fallback={fallback}\r\n          />\r\n        )\r\n      }\r\n      actions={\r\n        !loading && [\r\n          <Link to={`/product/${slug}`}>\r\n            <EyeTwoTone />\r\n            <br />\r\n            <p>View Product</p>\r\n          </Link>,\r\n          <Tooltip title={tooltip}>\r\n            <a onClick={handleAddToCart}>\r\n              <ShoppingTwoTone />\r\n              <br />\r\n              <p>Add to Cart</p>\r\n            </a>\r\n          </Tooltip>,\r\n        ]\r\n      }\r\n    >\r\n      {product && product.ratings && product.ratings.length > 0 ? (\r\n        <div className=\"pt-3 pb-2\">{showAverage(product)}</div>\r\n      ) : (\r\n        <p className=\"font-weight-light\" style={{ paddingTop: \"15px\" }}>\r\n          No rating yet\r\n        </p>\r\n      )}\r\n\r\n      <Meta\r\n        title={`${title} - IDR ${price} `}\r\n        description={description.length > 50 ? subDescription : description}\r\n      />\r\n      {/* <Meta className='py-2' description={`Color: ${color}`}/> */}\r\n    </Card>\r\n  );\r\n};\r\n\r\nexport default ProductCard;\r\n","C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\components\\cards\\Typewriter.js",[],"C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\components\\cards\\TypeWriter.js",[],"C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\components\\home\\NewArrivals.js",["298","299","300"],"import React, { useState, useEffect } from \"react\";\r\nimport { getProducts, getProductsCount } from \"../../functions/product\";\r\nimport ProductCard from \"../cards/Product