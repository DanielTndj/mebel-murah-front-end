\n            onChange={(event) => setPassword(event.target.value)}\r\n            autoFocus\r\n          />\r\n        </div>\r\n\r\n        <Button\r\n          onClick={sendEmailRegistration}\r\n          type=\"primary\"\r\n          block\r\n          shape=\"round\"\r\n          size=\"large\"\r\n        >\r\n          Complete Registration\r\n        </Button>\r\n      </form>\r\n    );\r\n  };\r\n\r\n  return (\r\n    <div className=\"container p-5\">\r\n      <div className=\"row\">\r\n        <div className=\"col-md-6 offset-md-3\">{completeRegistrationForm()}</div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default RegisterComplete;\r\n","C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\reducers\\index.js",[],"C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\reducers\\userReducer.js",[],"C:\\Users\\Daniel\\Documents\\Dokumen Kuliah\\Laporan\\TA\\Project\\Toko Mebel Murah Ecommerce\\client\\src\\pages\\auth\\ForgotPassword.js",["95"],"import React, { useState, useEffect } from \"react\";\r\nimport { auth } from \"../../firebase\";\r\nimport { toast } from \"react-toastify\";\r\nimport { useSelector } from \"react-redux\";\r\nimport Spinner from \"../../components/spinner/Spinner\";\r\nimport { Button } from \"antd\";\r\n\r\nconst ForgotPassword = ({ history }) => {\r\n  const [email, setEmail] = useState(\"\");\r\n  const [loading, setLoading] = useState(false);\r\n  const pattern = new RegExp(\r\n    /^((\"[\\w-\\s]+\")|([\\w-]+(?:\\.[\\w-]+)*)|(\"[\\w-\\s]+\")([\\w-]+(?:\\.[\\w-]+)*))(@((?:[\\w-]+\\.)*\\w[\\w-]{0,66})\\.([a-z]{2,6}(?:\\.[a-z]{2})?)$)|(@\\[?((25[0-5]\\.|2[0-4][0-9]\\.|1[0-9]{2}\\.|[0-9]{1,2}\\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\]?$)/i\r\n  );\r\n  const { user } = useSelector((state) => ({ ...state }));\r\n\r\n  useEffect(() => {\r\n    if (user 